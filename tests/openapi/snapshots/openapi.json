{
  "components": {
    "schemas": {
      "CheckResult": {
        "description": "Result of a single dependency check.\n\nAttributes:\n    name: Logical name of the dependency (e.g., \"db\", \"redis\").\n    status: One of {\"ok\",\"down\"} for the specific check.\n    detail: Optional human-readable detail (e.g., error message).\n    duration_ms: Milliseconds spent on the probe.",
        "properties": {
          "detail": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Detail"
          },
          "duration_ms": {
            "title": "Duration Ms",
            "type": "number"
          },
          "name": {
            "examples": [
              "db",
              "redis"
            ],
            "title": "Name",
            "type": "string"
          },
          "status": {
            "enum": [
              "ok",
              "down"
            ],
            "title": "Status",
            "type": "string"
          }
        },
        "required": [
          "name",
          "status",
          "duration_ms"
        ],
        "title": "CheckResult",
        "type": "object"
      },
      "ErrorEnvelope": {
        "additionalProperties": false,
        "description": "Canonical error envelope wrapping ErrorObject.",
        "examples": [
          {
            "error": {
              "code": "VALIDATION_ERROR",
              "details": {
                "field": "price",
                "min_value": 0
              },
              "http_status": 400,
              "message": "price must be >= 0",
              "trace_id": "c5a6a0c4-0c8d-4f0f-b6e6-8f1f0c2b7e7a"
            }
          }
        ],
        "properties": {
          "error": {
            "$ref": "#/components/schemas/ErrorObject",
            "description": "Structured error details."
          }
        },
        "required": [
          "error"
        ],
        "title": "ErrorEnvelope",
        "type": "object"
      },
      "ErrorObject": {
        "additionalProperties": false,
        "description": "Canonical error payload per API Standards ยง2.1.",
        "examples": [
          {
            "code": "VALIDATION_ERROR",
            "details": {
              "from": "2025-09-30",
              "to": "2025-01-01"
            },
            "http_status": 400,
            "message": "from must be <= to",
            "trace_id": "7e8a5d2e-2f8e-4a7a-8d2b-0e1f9e5c1234"
          }
        ],
        "properties": {
          "code": {
            "description": "Stable machine-readable error code (UPPER_SNAKE_CASE).",
            "title": "Code",
            "type": "string"
          },
          "details": {
            "anyOf": [
              {
                "additionalProperties": true,
                "type": "object"
              },
              {
                "type": "null"
              }
            ],
            "description": "Optional structured context; safe to expose to clients.",
            "title": "Details"
          },
          "http_status": {
            "description": "HTTP status code associated with this error.",
            "title": "Http Status",
            "type": "integer"
          },
          "message": {
            "description": "Human-readable, safe description of the error.",
            "title": "Message",
            "type": "string"
          },
          "trace_id": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "description": "Request correlation id echoed back to the client.",
            "title": "Trace Id"
          }
        },
        "required": [
          "code",
          "http_status",
          "message"
        ],
        "title": "ErrorObject",
        "type": "object"
      },
      "HealthState": {
        "description": "Overall health classification.",
        "enum": [
          "ok",
          "degraded",
          "down"
        ],
        "title": "HealthState",
        "type": "string"
      },
      "LivenessResponse": {
        "description": "Liveness response indicating the process is running.",
        "properties": {
          "status": {
            "const": "ok",
            "default": "ok",
            "title": "Status",
            "type": "string"
          }
        },
        "title": "LivenessResponse",
        "type": "object"
      },
      "PaginatedEnvelope": {
        "additionalProperties": false,
        "description": "Canonical paginated envelope for list responses.",
        "examples": [
          {
            "items": [
              {
                "company_id": "a3d8b3c8-9d9e-4a64-9b38-7e2b8cfd8c34",
                "currency": "USD",
                "revenue": "61800000000",
                "statement_date": "2024-12-31",
                "ticker": "MSFT"
              }
            ],
            "page": 1,
            "page_size": 50,
            "total": 1234
          }
        ],
        "properties": {
          "items": {
            "description": "Page contents (adapter DTOs).",
            "items": {},
            "title": "Items",
            "type": "array"
          },
          "page": {
            "description": "1-indexed page number.",
            "minimum": 1.0,
            "title": "Page",
            "type": "integer"
          },
          "page_size": {
            "description": "Items per page (bounded by API policy).",
            "maximum": 200.0,
            "minimum": 1.0,
            "title": "Page Size",
            "type": "integer"
          },
          "total": {
            "description": "Total number of records matching the current filter.",
            "minimum": 0.0,
            "title": "Total",
            "type": "integer"
          }
        },
        "required": [
          "page",
          "page_size",
          "total",
          "items"
        ],
        "title": "PaginatedEnvelope",
        "type": "object"
      },
      "ReadinessResponse": {
        "description": "Aggregated readiness response following a problem-details-like shape.\n\nAttributes:\n    status: Overall service status classification.\n    checks: Individual dependency check results.",
        "properties": {
          "checks": {
            "items": {
              "$ref": "#/components/schemas/CheckResult"
            },
            "title": "Checks",
            "type": "array"
          },
          "status": {
            "$ref": "#/components/schemas/HealthState"
          }
        },
        "required": [
          "status"
        ],
        "title": "ReadinessResponse",
        "type": "object"
      },
      "SuccessEnvelope": {
        "additionalProperties": false,
        "description": "Canonical success envelope for single-resource responses.",
        "examples": [
          {
            "data": {
              "id": "a3d8b3c8-9d9e-4a64-9b38-7e2b8cfd8c34",
              "ticker": "MSFT"
            }
          }
        ],
        "properties": {
          "data": {
            "description": "Adapter DTO or primitive value returned by the endpoint.",
            "title": "Data"
          }
        },
        "required": [
          "data"
        ],
        "title": "SuccessEnvelope",
        "type": "object"
      }
    }
  },
  "info": {
    "description": "Secure, governed financial data API.",
    "title": "Stacklion API",
    "version": "0.0.0"
  },
  "openapi": "3.1.0",
  "paths": {
    "/health/ready": {
      "get": {
        "description": "Aggregate dependency checks and return overall readiness.\n\nThe response status code is 200 when all checks are \"ok\", otherwise 503.\nIndividual check errors are captured in their `detail` field.\n\nArgs:\n    response: FastAPI response object used to mutate the status code.\n    probe: Injected health probe implementing dependency checks.\n\nReturns:\n    A `ReadinessResponse` summarizing dependency health.",
        "operationId": "health_readiness",
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ReadinessResponse"
                }
              }
            },
            "description": "Successful Response"
          },
          "503": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ReadinessResponse"
                }
              }
            },
            "description": "Service degraded or down"
          }
        },
        "summary": "Readiness"
      }
    },
    "/health/z": {
      "get": {
        "description": "Return a fast liveness signal (no external I/O).",
        "operationId": "health_liveness",
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/LivenessResponse"
                }
              }
            },
            "description": "Successful Response"
          }
        },
        "summary": "Liveness"
      }
    },
    "/healthz": {
      "get": {
        "description": "Simple process liveness endpoint.",
        "operationId": "_healthz_healthz_get",
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "additionalProperties": {
                    "type": "string"
                  },
                  "title": "Response  Healthz Healthz Get",
                  "type": "object"
                }
              }
            },
            "description": "Successful Response"
          }
        },
        "summary": " Healthz"
      }
    },
    "/v1/protected/ping": {
      "get": {
        "description": "Returns 200 when reachable. Requires valid bearer token when AUTH_ENABLED=true.",
        "operationId": "ping_v1_protected_ping_get",
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "additionalProperties": {
                    "type": "string"
                  },
                  "title": "Response Ping V1 Protected Ping Get",
                  "type": "object"
                }
              }
            },
            "description": "OK"
          },
          "401": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorEnvelope"
                }
              }
            },
            "description": "Unauthorized (missing/invalid auth)"
          },
          "403": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorEnvelope"
                }
              }
            },
            "description": "Forbidden (insufficient permissions)"
          }
        },
        "summary": "Auth check ping",
        "tags": [
          "Auth"
        ]
      }
    },
    "/v1/quotes/historical": {
      "get": {
        "description": "Returns historical OHLCV bars for the requested tickers and time window. Supports daily (1d) and intraday (1m) intervals. Results are paginated and conditionally cacheable via ETag (weak).",
        "operationId": "get_historical_quotes_v1_quotes_historical_get",
        "parameters": [
          {
            "description": "List of tickers",
            "in": "query",
            "name": "tickers",
            "required": true,
            "schema": {
              "description": "List of tickers",
              "items": {
                "type": "string"
              },
              "maxItems": 50,
              "minItems": 1,
              "title": "Tickers",
              "type": "array"
            }
          },
          {
            "description": "Start date (YYYY-MM-DD)",
            "in": "query",
            "name": "from_",
            "required": true,
            "schema": {
              "description": "Start date (YYYY-MM-DD)",
              "title": "From ",
              "type": "string"
            }
          },
          {
            "description": "End date (YYYY-MM-DD)",
            "in": "query",
            "name": "to",
            "required": true,
            "schema": {
              "description": "End date (YYYY-MM-DD)",
              "title": "To",
              "type": "string"
            }
          },
          {
            "description": "Bar interval (1d or 1m)",
            "in": "query",
            "name": "interval",
            "required": true,
            "schema": {
              "description": "Bar interval (1d or 1m)",
              "enum": [
                "1d",
                "1m"
              ],
              "title": "Interval",
              "type": "string"
            }
          },
          {
            "description": "1-based page",
            "in": "query",
            "name": "page",
            "required": false,
            "schema": {
              "default": 1,
              "description": "1-based page",
              "minimum": 1,
              "title": "Page",
              "type": "integer"
            }
          },
          {
            "description": "Items per page",
            "in": "query",
            "name": "page_size",
            "required": false,
            "schema": {
              "default": 50,
              "description": "Items per page",
              "maximum": 500,
              "minimum": 1,
              "title": "Page Size",
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PaginatedEnvelope"
                }
              }
            },
            "description": "Successful Response"
          },
          "400": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorEnvelope"
                }
              }
            },
            "description": "Bad request (validation or parameter)"
          },
          "401": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorEnvelope"
                }
              }
            },
            "description": "Unauthorized (missing/invalid auth)"
          },
          "403": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorEnvelope"
                }
              }
            },
            "description": "Forbidden (insufficient permissions)"
          },
          "404": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorEnvelope"
                }
              }
            },
            "description": "Not found"
          },
          "409": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorEnvelope"
                }
              }
            },
            "description": "Conflict"
          },
          "422": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorEnvelope"
                }
              }
            },
            "description": "Unprocessable content"
          },
          "429": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorEnvelope"
                }
              }
            },
            "description": "Rate limit exceeded"
          },
          "500": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorEnvelope"
                }
              }
            },
            "description": "Internal server error"
          },
          "503": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorEnvelope"
                }
              }
            },
            "description": "Service unavailable"
          }
        },
        "summary": "Get historical quotes (OHLCV bars)",
        "tags": [
          "Market Data"
        ]
      }
    }
  }
}
